create DATABASE gmap;

use gmap;

create TABLE stores(
id INT NOT NULL AUTO_INCREMENT PRIMARY KEY ,
s_name varchar(50) not null,
s_address varchar(150) not null,
s_type varchar(30) not null,
s_rating float,
markerColor varchar(10),
icon varchar(500),
lat float(10,8) not null,
lng float(10,8) not null
);


